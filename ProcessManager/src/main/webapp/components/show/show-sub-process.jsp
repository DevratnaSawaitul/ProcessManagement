<div class="main-content-subprocesses" id="main-content-subprocesses">
	<div class="subprocess-header">
		<h2>Sub-Processes</h2>
		<button class="add-subprocess-btn" title="Add New Sub-Process"
			onclick="addSubProcess()">+ Add Sub-Process</button>
	</div>

	<div class="table-container-subprocesses">
		<table class="subprocesses-table">
			<thead>
				<tr>
					<th>Process Name</th>
					<th>Sub-Process Name</th>
					<th>Active</th>
					<th>Action</th>
					<!-- Action Column -->
				</tr>
			</thead>
			<tbody id="subprocesses-table-body">
				<!-- Data rows will be dynamically inserted here -->
			</tbody>
		</table>
	</div>
</div>