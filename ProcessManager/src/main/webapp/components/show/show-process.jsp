<div class="main-content-processes" id="main-content-processes">
    <div class="process-header">
        <h2>Processes</h2>
        <button class="add-process-btn" title="Add New Process" onclick="addProcess()">+ Add Process</button>
    </div>
    <div class="table-container-processes">
        <table class="processes-table">
            <thead>
                <tr>
                    <th>Process Name</th>
                    <th>Active</th>
                    <th>Action</th> <!-- New Action Column -->
                </tr>
            </thead>
            <tbody id="processes-table-body">
                <!-- Data rows will be dynamically inserted here -->
            </tbody>
        </table>
    </div>
</div>
